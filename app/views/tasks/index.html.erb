<h1>Tasks List</h1>
<p>Find me in app/views/tasks/index.html.erb</p>

<% @tasks.each do |task| %>
  <h2><%= task.name %></h2>
    <p><%= link_to "Delete!!", task_path(task), method: :delete %></p>
    <p><%= link_to "Show Tasks", task_path(task) %></p>
    <p><%= link_to "Edit Tasks", edit_task_path(task) %></p>
<% end %>

<%= link_to "Make a new Task", new_task_path %>

